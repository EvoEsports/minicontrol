{
    "linter": {
        "rules": {
            "suspicious": {
                "noExplicitAny": "off"
            },
            "complexity": {
                "useLiteralKeys": "off"
            }
        }
    },
    "files": {
        "include": [
            "core/**/*.ts",
            "userdata/plugins/**/*.ts"
        ],
        "ignore": [
            "test",
            "node_modules"
        ]
    }
}
