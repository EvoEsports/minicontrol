{
  "scripts": {
    "dev": "drizzle-kit generate:sqlite --config=drizzle.config.ts && tsx --env-file=.env --watch run core/minicontrol.ts",
    "dev2": "drizzle-kit generate:sqlite --config=drizzle.config.ts && tsx --env-file=.env2 --watch run core/minicontrol.ts",
    "start": "drizzle-kit generate:sqlite --config=drizzle.config.ts && tsx --env-file=.env core/minicontrol.ts",
